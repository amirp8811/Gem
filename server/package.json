{
  "name": "gravity-jewelry-backend",
  "version": "1.0.0",
  "description": "Backend API for Gravity Jewelry Marketplace",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup-db": "node scripts/setup-database.js",
    "seed-db": "node scripts/seed-database.js",
    "test": "jest",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "reset-db": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "tunnel": "ngrok http 5000",
    "tunnel:subdomain": "ngrok http 5000 --subdomain=gravity-jewelry",
    "tunnel:secure": "ngrok http 5000 --basic-auth=admin:Gravity2024!@#",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "dev:public": "concurrently \"npm run dev\" \"npm run tunnel:subdomain\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "knex": "^3.0.1",
    "stripe": "^14.7.0",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "redis": "^4.6.10",
    "csurf": "^1.11.0",
    "cookie-parser": "^1.4.6",
    "validator": "^13.11.0",
    "isomorphic-dompurify": "^2.6.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "jewelry",
    "marketplace",
    "api",
    "express",
    "mysql"
  ],
  "author": "Gravity Team",
  "license": "MIT"
}
